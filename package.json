{
  "name": "@webqit/webqit.io",
  "version": "0.0.1",
  "description": "The webqit.io website",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "postversion": "git push && git push --tags",
    "build-css": "tailwindcss -c ./public/css/tailwind.config.cjs -i ./public/css/tailwind.dev.css -o ./public/css/tailwind.css",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && webflo build",
    "start": "npm run build-css && webflo start"
  },
  "keywords": [
    "WebQit Tooling",
    "WebQit Cloud",
    "WebQit Community"
  ],
  "author": "Oxford Harrison <oxharris.dev@gmail.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@webqit/backpack": "0.0.3",
    "@webqit/observer": "^1.7.5",
    "@webqit/obsv-collection": "^0.0.16",
    "@webqit/play-icon": "0.0.2",
    "@webqit/play-sequence": "^0.1.7",
    "@webqit/pseudo-browser": "^0.3.12",
    "@webqit/util": "^0.7.1",
    "bootstrap-icons": "^1.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.4",
    "tailwind-scrollbar": "^1.3.1",
    "tailwindcss": "^3.0.7"
  }
}
